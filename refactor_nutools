#!/bin/bash
for file in `find * -type f`;do sed -i.bak -f ./rename_headerincludes_nutools.sed $file;done
for file in `find * -name CMakeLists.txt`;do sed -i.bak -f ./rename_libraryincludes_nutools.sed $file;done
for file in `find * -name GNUmakefile`;do sed -i.bak -f ./rename_libraryincludes_nutools.sed $file;done
